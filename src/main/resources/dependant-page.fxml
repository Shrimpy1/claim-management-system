<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rmit.furtherprog.claimmanagementsystem.controller.DependantPageController">
    <Label fx:id="welcomeLabel"  text="Welcome" />
    <Button fx:id="logoutButton" text="Logout" onAction="#handleLogoutButtonAction" />
    <VBox alignment="CENTER" spacing="10">
        <Label text="Our Services" />
        <Button fx:id="selfInfoButton" text="Self Information" onAction="#handleSelfInfoButton" />
        <Button fx:id="viewClaimsButton" text="View Claims" onAction="#handleViewClaimsButton" />
    </VBox>
    <VBox fx:id="additionalContentContainer" alignment="CENTER" spacing="10"/>
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
</VBox>
